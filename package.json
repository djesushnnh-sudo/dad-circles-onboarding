{
  "name": "dad-circles-onboarding",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "cross-env VITE_GEMINI_API_KEY=your_api_key_here vite",
    "dev:server": "node server.js",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && firebase deploy",
    "deploy:hosting": "npm run build && firebase deploy --only hosting",
    "deploy:preview": "npm run build && firebase hosting:channel:deploy staging",
    "deploy:live": "firebase hosting:clone dad-circles:staging dad-circles:live",
    "emulator": "firebase emulators:start",
    "emulator:seed": "firebase emulators:start --import=./emulator-data --export-on-exit=./emulator-data",
    "seed:test": "tsx scripts/seedTestUsers.ts",
    "clean:test": "tsx scripts/cleanTestUsers.ts",
    "logs": "node scripts/readLogs.js",
    "logs:matching": "node scripts/readLogs.js matching",
    "logs:database": "node scripts/readLogs.js database"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase": "^12.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.11.0",
    "resend": "^6.7.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "cross-env": "^10.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
